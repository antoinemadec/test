use std::{
    fs::File,
    io::{BufRead, BufReader},
};

fn read(filename: &str) -> Vec<String> {
    let io = File::open(filename).unwrap();
    let br = BufReader::new(io);
    br.lines().map(|line| line.unwrap()).collect()
}

fn p1(filename: &str) -> i32 {
    for line in read(filename) {
        println!()
    }
    0
}

fn main() {
    println!("p1 {}", p1("src/input.txt"));
    // println!("p2 {}", p2("src/input.txt"));
}
